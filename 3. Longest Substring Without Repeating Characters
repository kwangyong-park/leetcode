class Solution {
     public int lengthOfLongestSubstring(String s) {
      
        char[] chars = s.toCharArray();
         
        int max = 0;
        for(int i = 0; i < chars.length; i++) {
            Set<Character> set = new HashSet();
            int count = 0;
            for(int j = i; j < chars.length; j++) {
                if(set.contains(chars[j])) {
                    break;
                }
                count++;
                set.add(chars[j]);
            }
            if(count > max) {
                max = count;
            }
        }
         return max;
    }
}
